{
  "name": "reverse-shell",
  "version": "1.0.0",
  "description": "Reverse Shell as a Service - https://resh.now.sh",
  "main": "src/index.js",
  "scripts": {
    "start": "micro",
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "now --public --env NODE_ENV=production && now alias"
  },
  "now": {
    "alias": [
      "resh"
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "author": "Virink"
        },
        "dest": "/api/reverseshell/$1"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virink/reverse-shell.git"
  },
  "keywords": [
    "reverse",
    "shell",
    "pentest",
    "ctf"
  ],
  "author": "Virink <virink@outlook.com> (https://www.virzz.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virink/reverse-shell/issues"
  },
  "homepage": "https://github.com/virink/reverse-shell",
  "dependencies": {},
  "devDependencies": {
    "@now/node": "^1.6.1"
  }
}